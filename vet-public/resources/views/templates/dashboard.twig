        function AgregarIniciativa(id_iniciativa) {
            var id = "#" + id_iniciativa;
            event.preventDefault();
            Swal.fire({
                title: '¿Está seguro?',
                icon: 'warning',
                text: 'Solo puede escoger tres iniciativas.',
                showCancelButton: true,
                confirmButtonColor: '#3085d6',
                cancelButtonColor: '#d33',
                confirmButtonText: 'Sí, guardar!'
            }).then((result) => {
                if (result.value) {
                    var resp = GuardarIniciativa(id_iniciativa);
                    if (resp[0] == "success") {
                        Swal.fire({
                            icon: 'success',
                            title: '¡Éxito!',
                            text: resp[1],
                        }).then((result) => {
                            if (result.value) {
                                $(id).html('<i class="fa fa-check"></i>');
                                $(id).addClass("btn-success");
                                $("#listini").html("");
                                IniciativasSeleccionadas();
                            }
                        });
                    } else if (resp[0] == "lleno") {
                        Swal.fire({
                            icon: 'Atención',
                            title: '¡Error!',
                            text: resp[1],
                        })
                        $(id).html('<i class="fa fa-hand-o-up"></i>');
                    } else if (resp[0] == "limite") {
                        Swal.fire({
                            icon: 'warning',
                            title: '¡Atención!',
                            text: resp[1],
                        })
                        $(id).html('<i class="fa fa-hand-o-up"></i>');
                    } else {
                        Swal.fire({
                            icon: 'error',
                            title: '¡Error!',
                            text: resp[1],
                        })
                        $(id).html('<i class="fa fa-hand-o-up"></i>');
                    }
                }
            })
        }
