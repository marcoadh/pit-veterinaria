    {% include 'templates/partials/footer.twig' %}
    <script>

        function NuevaMascota() {
            $("#FormMascota")[0].reset();
            $("#seccionfoto").css("display", "none");
            $('#accion').val('nuevo');
            $('#AgregarMascotas').modal('show');
        }
        
        function NuevaPedido() {
            $("#FormPedido")[0].reset();
            $('#AgregarPedido').modal('show');
        }
        
        function VerHistorial() {
            $("#FormHistorial")[0].reset();
            $('#VerHistorial').modal('show');
        }
        
        function EditarMascota(codigo) {
            $("#seccionfoto").css("display", "block");
            var codigo = codigo;
            $('#AgregarMascotas').modal('show');
            $('#codigo').val(codigo);
            $('#accion').val('editar');
            return $.ajax({
                type: "get",
            url: '{{ base_url() }}/mascota/editar',
                data: {
                    codigo: codigo
                },
                success: function (data) {
                    $.each(data, function (i, item) {
                        $('#nombre').val(item.nombre);
                        $('#edad').val(item.edad);
                        $('#fotooriginal').attr('src', '{{base_url()}}/' + item.foto);
                        $('#estado').val(item.estado);
                        $('#descripcion').val(item.descripcion);
                        $('#id_tipomascota').val(item.id_tipomascota);
                    });
                },
                error: function () {
                }
            });

        }

        function GuardarMascota() {
            var formData = new FormData($("#FormMascota")[0]);
            var resultado = "";
            $.ajax({
            url: "{{ base_url() }}/mascota/registrar",
                async: false,
                type: 'POST',
                data: formData,
                contentType: false,
                enctype: 'multipart/form-data',
                processData: false,
                dataType: 'json',
                success: function (datos) {
                    resultado = datos.response;
                    mensaje = datos.message;
                }
            });
            return [resultado, mensaje];
        }
        
         function GuardarPedido() {
            var formData = new FormData($("#FormPedido")[0]);
            var resultado = "";
            $.ajax({
            url: "{{ base_url() }}/pedido/registrar",
                async: false,
                type: 'POST',
                data: formData,
                contentType: false,
                enctype: 'multipart/form-data',
                processData: false,
                dataType: 'json',
                success: function (datos) {
                    resultado = datos.response;
                    mensaje = datos.message;
                }
            });
            return [resultado, mensaje];
        }


        $(".registrarmascota").on("click", function (event) {

            event.preventDefault();
           
            var nombre = $('#nombre').val();
            var tipo = $('#id_tipomascota').val();
            var edad = $('#edad').val();
            var descripcion = $('#descripcion').val();
            var estado = $('#estado').val();
            var foto = $('#foto').val();
            
            if (nombre == "") {
                Swal.fire({
                    icon: 'warning',
                    title: '¡Advertencia!',
                    text: 'Debe ingresar el nombre de la mascota.',
                })
            } else {
                if (tipo == "") {
                    Swal.fire({
                        icon: 'warning',
                        title: '¡Advertencia!',
                        text: 'Debe seleccionar el tipo de mascota.',
                    })
                } else {
                   if (edad == "") {
                        Swal.fire({
                            icon: 'warning',
                            title: '¡Advertencia!',
                            text: 'Debe ingresar la fecha de nacimiento de la mascota.',
                        })
                    } else {
                         if (descripcion == "") {
                            Swal.fire({
                                icon: 'warning',
                                title: '¡Advertencia!',
                                text: 'Debe ingresar la descrición de la mascota.',
                            })
                        } else {
                             if (estado == "") {
                                Swal.fire({
                                    icon: 'warning',
                                    title: '¡Advertencia!',
                                    text: 'Debe seleccionar el estado.',
                                })
                            } else {
                                if (foto == "" && accion == "nuevo") {
                                    Swal.fire({
                                        icon: 'warning',
                                        title: '¡Advertencia!',
                                        text: 'Debe agregar una foto.',
                                    })
                                } else {
                                    Swal.fire({
                                        title: '¿Está seguro?',
                                        icon: 'warning',
                                        showCancelButton: true,
                                        confirmButtonColor: '#3085d6',
                                        cancelButtonColor: '#d33',
                                        confirmButtonText: '¡Sí, guardar!'
                                    }).then((result) => {
                                        if (result.value) {
                                            var resp = GuardarMascota();
                                            if (resp[0] == "success") {
                                                Swal.fire({
                                                    icon: 'success',
                                                    title: '¡Éxito!',
                                                    text: resp[1],
                                                }).then((result) => {
                                                    if (result.value) {
                                                        $('#AgregarMascotas').modal('hide');
                                                        location.reload();
                                                    }
                                                });
                                            } else {
                                                Swal.fire({
                                                    icon: 'error',
                                                    title: '¡Error!',
                                                    text: resp[1],
                                                })
                                            }
                                        }
                                    })
                                }
                            }
                        }
                    }
                }
            }
        });
        
          $(".registrarpedido").on("click", function (event) {

            event.preventDefault();

            var mascota = $('#code_mascota').val();
            var pedido = $('#code_pedido').val();

            if (mascota == "") {
                Swal.fire({
                    icon: 'warning',
                    title: '¡Advertencia!',
                    text: 'Debe seleccionar a una mascota.',
                })
            } else {
                if (pedido == "") {
                    Swal.fire({
                        icon: 'warning',
                        title: '¡Advertencia!',
                        text: 'Debe seleccionar el tipo de pedido.',
                    })
                } else {
                    Swal.fire({
                        title: '¿Está seguro?',
                        icon: 'warning',
                        showCancelButton: true,
                        confirmButtonColor: '#3085d6',
                        cancelButtonColor: '#d33',
                        confirmButtonText: '¡Sí, guardar!'
                    }).then((result) => {
                        if (result.value) {
                            var resp = GuardarPedido();
                            if (resp[0] == "success") {
                                Swal.fire({
                                    icon: 'success',
                                    title: '¡Éxito!',
                                    text: resp[1],
                                }).then((result) => {
                                    if (result.value) {
                                        $('#AgregarPedido').modal('hide');
                                        window.location.href = "pedido";
                                    }
                                });
                            } else {
                                Swal.fire({
                                    icon: 'error',
                                    title: '¡Error!',
                                    text: resp[1],
                                })
                            }
                        }
                    })
                }   
            }
        });



        $(document).ready(function () {

            // Translated
            $('.dropify-es').dropify({
                messages: {
                    default: 'Arrastra y suelta un archivo aquí o haz clic',
                    replace: 'Arrastre y suelte un archivo o haga clic para reemplazar',
                    remove: 'Suprimir',
                    error: 'Lo sentimos, el archivo es demasiado grande'
                }
            });

            // Used events
            var drEvent = $('#input-file-events').dropify();

            drEvent.on('dropify.beforeClear', function (event, element) {
                return confirm("Do you really want to delete \"" + element.file.name + "\" ?");
            });

            drEvent.on('dropify.afterClear', function (event, element) {
                alert('File deleted');
            });

            drEvent.on('dropify.errors', function (event, element) {
                console.log('Has Errors');
            });

            var drDestroy = $('#input-file-to-destroy').dropify();
            drDestroy = drDestroy.data('dropify')
            $('#toggleDropify').on('click', function (e) {
                e.preventDefault();
                if (drDestroy.isDropified()) {
                    drDestroy.destroy();
                } else {
                    drDestroy.init();
                }
            })

        });



